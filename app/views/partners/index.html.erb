<h1> Partners list</h1>

<ul>
<% @partners.each do |partner| %>
  <li> <%= partner.name %> <%= link_to "show the partner", partner_path(partner) %></li>
<% end %>
</ul>


<div id="map" data-markers="<%= @markers.to_json %>"></div>


<% content_for :after_js do %>
  <script type="text/javascript" src="http://maps.google.com/maps/api/js?key=<%= ENV['GOOGLE_MAP_KEY']%>&callback=initMap" async defer></script>
<% end %>

